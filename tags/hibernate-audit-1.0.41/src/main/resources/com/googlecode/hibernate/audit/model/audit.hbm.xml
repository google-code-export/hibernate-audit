		<bag name="auditTypeAttributes" cascade="all" inverse="true"
			lazy="true">
			<cache usage="nonstrict-read-write" region="com.googlecode.hibernate.audit.model.clazz" />
			<key column="`AUDIT_TYPE_ID`" />
			<one-to-many
